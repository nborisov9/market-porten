{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"max-len": "off",
		"linebreak-style": "off",
		"no-unused-vars": "off",
		"indent": "off",
		"no-tabs": "off",
		"no-mixed-spaces-and-tabs": "off",
		"eol-last": "off",
		"require-jsdoc": "off",
		"no-multiple-empty-lines": "off",
		"operator-linebreak": "off"
		// "padded-blocks": "off",
		// "no-trailing-spaces": "off"
	},
		"extends": ["eslint:recommended", "google"]
}